use "std/libc/proc.bl" @proc
use "std/backtrace/panic.bl" {panic}

enum MyEnum {
    A(int)
    B(bool)
    C
}

fn main() {
    let mut value = MyEnum::A(5)
    match &mut value {
    case let MyEnum::A(v):
        v^ *= 2
    case let other:
        other^ = MyEnum::C
    }

    match value {
    case let MyEnum::A(v) if v == 10:
    case: 
        panic("expected MyEnum::(10)")
    }

    @proc.exit(0)
}